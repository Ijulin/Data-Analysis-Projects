libname proj2 '/home/u59305518/BUMK742/Project2';


data temp;  
set proj2.printads;

/* create a dummy variable for the L'Oreal ad */
/* Because the variable BRAND is a character variable, we need to use the */
/* quotation marks when referring to specific values of it, LOreal in this case */
if brand='LOreal' then loreal=1;
else loreal=0;
Run;
/* Get descriptive stats */
proc means;
var brand_fix pic_fix brand_size pic_size page_num; /* list any variables you are interested */

/* Get correlation matrix */
proc corr;
var brand_fix pic_fix brand_size pic_size page_num; 

/* Get a list of all brand names via frequency tabulation */
proc freq;
table brand;

/* Poisson Regression Models */

/* For BRAND_FIX: fixation counts of the brand element */ 
proc genmod data=temp;
     model brand_fix=brand_size page_pos page_num loreal/
           dist=poisson link=log;
     title 'Poission Regression for brand fixation count';

/* For PIC_FIX: fixation counts of the pictorial element */ 
proc genmod data=temp;
     model pic_fix=pic_size page_pos page_num loreal/
           dist=poisson link=log;
     title 'Poission Regression for pictorial fixation count';

/* Binary Logit Model */
/* (Note that this model is also called logistic regression. */
/* In SAS, you can also estimate this model using another    */
/* procedure which is specific for logistic regressions.) */ 
/* Important note:           */
/* By default, SAS estimates the probability that Y=the lower value, */
/* which is RECALL_ACCU = 0 in our case. */
/* Need to specify DESCENDING in the PROC GENMOD statement for SAS */
/* to estimate the probability RECALL_ACCU=1 */

proc genmod data=temp descending;
     model recall_accu=brand_fix pic_fix page_pos page_num recall_time brand_size*recall_time page_pos*recall_time/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1';
run;



/*Q4*/
proc genmod data=temp descending;
     model recall_accu=page_pos page_num/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 page position';
run;
/*Q6*/
proc genmod data=temp descending;
     model recall_accu=page_pos page_num brand_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 page_pos page_num brand_size no pic_size';
RUN;


/*Loreal */
proc genmod data=temp descending;
     model recall_accu=loreal Recall_time /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 w/ loreal';
RUN;



/* no interaction effect*//*page_pos Intercept insignificant*/
proc genmod data=temp descending;
     model recall_accu=page_pos page_num brand_size pic_size brand_fix pic_fix recall_time/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 all variable';
RUN;

/* no interaction effect*//*no page_pos    Intercept insignificant*/
proc genmod data=temp descending;
     model recall_accu=page_num brand_size pic_size brand_fix pic_fix Recall_time/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;

/* no interaction effect*//*no page_pos page_num   *//*all significant*/
proc genmod data=temp descending;
     model recall_accu=  brand_size  pic_size brand_fix pic_fix Recall_time/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;


/*interaction effect  recall_time*page_num     Intercept RECALL_TIME insignificant*/ /*try others but still*/
proc genmod data=temp descending;
     model recall_accu=recall_time page_num brand_size pic_size brand_fix pic_fix page_num*recall_time /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 add interaction effect';
RUN;

/*interaction effect  recall_time*brand_size    Intercept recall_time*brand_size insignificant*//*try others but still*/
proc genmod data=temp descending;
     model recall_accu=recall_time  page_num brand_size pic_size brand_fix  pic_fix recall_time*brand_size /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 add interaction effect';
RUN;

/*interaction effect  recall_time*pic_size      Intercept RECALL_TIME pic_size recall_time*pic_size insignificant*/
proc genmod data=temp descending;
     model recall_accu=recall_time  page_num pic_size brand_size brand_fix  pic_fix recall_time*pic_size  /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 add interaction effect';
RUN;

/*interaction effect  recall_time*pic_fix       Intercept pic_fix  recall_time*pic_fix insignificant*/
proc genmod data=temp descending;
     model recall_accu=recall_time  page_num pic_size brand_size brand_fix  pic_fix recall_time*pic_fix  /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 add interaction effect';
RUN;

/* interaction effect *//*all*/
proc genmod data=temp descending;
     model recall_accu=  brand_size brand_size*recall_time page_pos page_num brand_fix  pic_fix  pic_size Recall_time brand_fix*recall_time pic_fix*recall_time recall_time*page_num recall_time*page_pos  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;

/* interaction effect *//*X brandsize*/
proc genmod data=temp descending;
     model recall_accu=  page_pos page_num brand_fix  pic_fix  pic_size Recall_time brand_fix*recall_time pic_fix*recall_time recall_time*page_num recall_time*page_pos  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;

/* interaction effect *//*x page_pos*/
proc genmod data=temp descending;
     model recall_accu=  brand_size brand_size*recall_time  page_num brand_fix  pic_fix  pic_size Recall_time brand_fix*recall_time pic_fix*recall_time recall_time*page_num  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;

/* interaction effect *//*x page_num*/
proc genmod data=temp descending;
     model recall_accu=  brand_size brand_size*recall_time page_pos  brand_fix  pic_fix  pic_size Recall_time brand_fix*recall_time pic_fix*recall_time recall_time*page_pos  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
 
/* interaction effect *//*x brand_fix*/
proc genmod data=temp descending;
     model recall_accu=  brand_size brand_size*recall_time page_pos page_num   pic_fix  pic_size Recall_time  pic_fix*recall_time recall_time*page_num recall_time*page_pos  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
 
/* interaction effect *//*x pic_fix*/
proc genmod data=temp descending;
     model recall_accu=  brand_size brand_size*recall_time page_pos page_num brand_fix    pic_size Recall_time brand_fix*recall_time  recall_time*page_num recall_time*page_pos  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
 
/* interaction effect *//*x pic_size*/
proc genmod data=temp descending;
     model recall_accu=  brand_size brand_size*recall_time page_pos page_num brand_fix  pic_fix   Recall_time brand_fix*recall_time pic_fix*recall_time recall_time*page_num recall_time*page_pos/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;


/* interaction effect *//*X brandsize page_pos*/
proc genmod data=temp descending;
     model recall_accu=   page_num brand_fix  pic_fix  pic_size Recall_time brand_fix*recall_time pic_fix*recall_time recall_time*page_num   recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;

/* interaction effect *//*X brandsize page_num*/
proc genmod data=temp descending;
     model recall_accu= page_pos  brand_fix  pic_fix  pic_size Recall_time brand_fix*recall_time pic_fix*recall_time  recall_time*page_pos  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
 
 /* interaction effect *//*X brandsize pic_size*/
proc genmod data=temp descending;
     model recall_accu=   page_pos page_num brand_fix  pic_fix   Recall_time brand_fix*recall_time pic_fix*recall_time recall_time*page_num recall_time*page_pos /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
 
/* interaction effect *//*X brandsize brand_fix*/
proc genmod data=temp descending;
     model recall_accu=page_pos page_num pic_fix  pic_size Recall_time  pic_fix*recall_time recall_time*page_num recall_time*page_pos  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN; 

/* interaction effect *//*X brandsize pic_fix*/
proc genmod data=temp descending;
     model recall_accu= page_pos page_num brand_fix   pic_size Recall_time brand_fix*recall_time  recall_time*page_num recall_time*page_pos  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;

/* interaction effect *//*X brandsize page_pos pic_size pic_fix*/
proc genmod data=temp descending;
     model recall_accu=   page_num brand_fix   Recall_time brand_fix*recall_time  recall_time*page_num  /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
/* interaction effect *//*x page_pos pic_size*/
proc genmod data=temp descending;
     model recall_accu=  brand_size brand_size*recall_time  page_num brand_fix  pic_fix   Recall_time brand_fix*recall_time pic_fix*recall_time recall_time*page_num  /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;

/* interaction effect *//*x page_pos pic_fix*/
proc genmod data=temp descending;
     model recall_accu=  brand_size brand_size*recall_time  page_num brand_fix   pic_size Recall_time brand_fix*recall_time  recall_time*page_num  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
/* interaction effect *//*x page_pos brand_fix*/
proc genmod data=temp descending;
     model recall_accu=  brand_size brand_size*recall_time  page_num  pic_fix  pic_size Recall_time pic_fix*recall_time recall_time*page_num  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
/* interaction effect *//*x page_pos page_num*/
proc genmod data=temp descending;
     model recall_accu=  brand_size brand_size*recall_time  brand_fix  pic_fix  pic_size Recall_time brand_fix*recall_time pic_fix*recall_time  recall_time*pic_size/
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;

/* interaction effect *//*O*/
proc genmod data=temp descending;
     model recall_accu=    brand_fix   Recall_time brand_fix*recall_time  /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
/*O*/
proc genmod data=temp descending;
     model recall_accu=    brand_fix   Recall_time brand_fix*recall_time pic_fix /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
/*O except intercept*/
proc genmod data=temp descending;
     model recall_accu=    brand_fix   Recall_time brand_fix*recall_time pic_fix page_num /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
/*O except intercept*/
proc genmod data=temp descending;
     model recall_accu=    brand_fix   Recall_time brand_fix*recall_time pic_fix page_num pic_size /
	       dist=binomial link=logit;
     title 'Binary Logit Model for Recall Accuracy = 1 ';
RUN;
